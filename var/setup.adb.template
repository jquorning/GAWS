   end Get_Uname_N;


   function Get_Uname_P return String is
   begin
      return "@_UNAME_OPT_P_@";
   end Get_Uname_P;


   function Get_Uname_R return String is
   begin
      return "@_UNAME_OPT_R_@";
   end Get_Uname_R;


   function Get_Uname_S return String is
   begin
      return "@_UNAME_OPT_S_@";
   end Get_Uname_S;


end Setup;
